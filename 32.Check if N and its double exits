class Solution {
    public boolean checkIfExist(int[] arr) {
        int l=arr.length;
        for(int i=0;i<l;i++){
            for(int j=i+1;j<l;j++){
                if(arr[i]==2*arr[j])return true;
                if(arr[j]==2*arr[i])return true;
            }
        }
        return false;
    }

}
