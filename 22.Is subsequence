class Solution {
    public boolean isSubsequence(String s, String t) {
        int j=0,sl=s.length(),tl=t.length();
        if(sl>0){
        for(int i=0;i<tl;i++){
            if(t.charAt(i)==s.charAt(j))j++;
        if(j==sl)return true;

        }
        }
        if(sl==0)return true;
        return false;
    }
}
